// CPU-bound: Prime number computation
{
    const limit = 100000;
    const primes = [];
    for (let i = 2; i < limit; i++) {
      let isPrime = true;
      for (let j = 2; j * j <= i; j++) {
        if (i % j === 0) {
          isPrime = false;
          break;
        }
      }
      if (isPrime) primes.push(i);
    }
    // console.log("Prime count:", primes.length);
  }
  
